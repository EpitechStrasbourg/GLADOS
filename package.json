{
  "name": "discordjs-template-ts",
  "description": "Template for a discord.js bot with TypeScript",
  "version": "14.14.1-0.1.5",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsup && tsc-alias",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "lint:fix": "eslint --fix ./src/**/*.ts",
    "lint": "eslint ./src/**/*.ts",
    "db:migrate": "npm run build && node dist/database/database.migrate.js",
    "prepare": "husky",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "chalk": "4.1.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "umzug": "^2.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-airbnb": "^0.0.1-security",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "globals": "^15.0.0",
    "typescript-eslint": "^7.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.20",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@types/umzug": "^2.3.7",
    "cross-env": "^7.0.3",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "tsc-alias": "^1.8.8",
    "tslib": "^2.6.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.0"
  }
}
